    {% endif %}
    "{{ipsum.key}}": [{
        "title": "{{ipsum.title}}",
        "url": "{{site.url}}/json/{{ipsum.key}}.json",

  "language": [
    {
        {% assign j=0 %}{% for language in ipsum.language %} {% assign j=j | plus:1 %}
        {% if j > 1 %}
            ,
        {% endif %}
            "name": "{{language.name}}"
        {% endfor %}
    }
  ]
    }]
{% endfor %} }